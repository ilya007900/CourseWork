<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:PupilReactionModule.Styles">

    <Style x:Key="TableProperty" TargetType="TextBlock">
        <Setter Property="TextWrapping" Value="WrapWithOverflow"></Setter>
        <Setter Property="Margin" Value="5 3"></Setter>
    </Style>
    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="Pack://application:,,,/WpfApp;component/Styles/Style.xaml"></ResourceDictionary>
    </ResourceDictionary.MergedDictionaries>
</ResourceDictionary>